{
  "homepage": "/mockServer/static",
  "name": "mock-server-gui",
  "version": "0.0.3",
  "private": false,
  "bin": {
    "mock-server-start": "node-server-dist/index.js"
  },
  "files": [
    "build",
    "node-server-dist"
  ],
  "dependencies": {
    "mime-types": "^2.1.25"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-app-rewired start",
    "start:server": "ts-node-dev --project node-server/tsconfig.json -- node-server/index.ts",
    "build:server": "tsc node-server/* node-server/**/* --outDir node-server-dist",
    "build": "react-app-rewired build && yarn build:server",
    "build:analyze": "yarn run build && yarn run analyze",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "node-sass": "^4.12.0",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-json-view": "^1.19.1",
    "react-router-dom": "^5.1.1",
    "react-scripts": "3.1.2",
    "source-map-explorer": "^2.1.1",
    "typescript": "3.6.3",
    "@types/jest": "24.0.18",
    "@types/mime-types": "^2.1.0",
    "@types/node": "12.7.8",
    "@types/react": "16.9.3",
    "@types/react-dom": "16.9.1",
    "antd": "^3.24.2",
    "axios": "^0.19.0",
    "babel-plugin-import": "^1.12.2",
    "customize-cra": "^0.8.0",
    "@types/react-router-dom": "^5.1.0",
    "react-app-rewired": "^2.1.3",
    "ts-node-dev": "^1.0.0-pre.43"
  }
}
